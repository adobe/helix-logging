{
  "name": "@adobe/helix-logging",
  "version": "1.1.14",
  "private": true,
  "description": "An OpenWhisk Action that sets up logging to Google BigQuery for a Fastly Service Config",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 90 --statements 100 --lines 100 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment",
    "semantic-release": "semantic-release",
    "junit": "mocha --exit -R mocha-junit-reporter",
    "lint": "./node_modules/.bin/eslint .",
    "build": "wsk-builder -v --entryFile index.js",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/helix-logging.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-logging/issues"
  },
  "homepage": "https://github.com/adobe/helix-logging#readme",
  "dependencies": {
    "@adobe/fastly-native-promises": "1.13.0",
    "@adobe/helix-shared": "3.0.0",
    "@adobe/helix-status": "5.0.1",
    "@google-cloud/bigquery": "4.2.1",
    "epsagon": "1.37.0",
    "googleapis": "43.0.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/helix-testutils": "0.2.0",
    "@adobe/openwhisk-action-builder": "2.1.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
    "@semantic-release/exec": "3.3.7",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.4.3",
    "@semantic-release/release-notes-generator": "7.3.0",
    "ajv": "6.10.2",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "codecov": "3.6.1",
    "eslint": "6.5.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "istanbul": "1.1.0-alpha.1",
    "junit-report-builder": "1.3.3",
    "mocha": "6.2.1",
    "mocha-junit-reporter": "1.23.1",
    "mocha-parallel-tests": "2.2.2",
    "nock": "11.3.5",
    "nyc": "14.1.1",
    "semantic-release": "15.13.24"
  },
  "wsk": {
    "name": "helix-services/logging@${version}"
  },
  "statuspage": {
    "name": "Helix Logging",
    "group": "Development"
  },
  "newrelic": {
    "group_policy": "Development Repeated Failure"
  }
}
