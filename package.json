{
  "name": "@adobe/helix-logging",
  "version": "1.1.7",
  "private": true,
  "description": "An OpenWhisk Action that sets up logging to Google BigQuery for a Fastly Service Config",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 95 --statements 100 --lines 100 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment",
    "semantic-release": "semantic-release",
    "junit": "mocha --exit -R mocha-junit-reporter",
    "lint": "./node_modules/.bin/eslint .",
    "build": "wsk-builder -v --entryFile index.js",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/helix-logging.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-logging/issues"
  },
  "homepage": "https://github.com/adobe/helix-logging#readme",
  "dependencies": {
    "@adobe/fastly-native-promises": "^1.11.0",
    "@adobe/helix-pingdom-status": "^3.1.1",
    "@adobe/helix-shared": "^2.1.1",
    "@google-cloud/bigquery": "^4.1.8",
    "epsagon": "^1.32.0",
    "googleapis": "^41.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "^1.0.0",
    "@adobe/openwhisk-action-builder": "^1.3.1",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/exec": "^3.3.6",
    "@semantic-release/git": "^7.0.14",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "ajv": "^6.10.2",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^1.1.0-alpha.1",
    "junit-report-builder": "^1.3.3",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-parallel-tests": "^2.0.3",
    "nock": "^10.0.6",
    "nyc": "^14.0.0",
    "semantic-release": "^15.13.19"
  },
  "wsk": {
    "name": "helix-services/logging@${version}"
  }
}
